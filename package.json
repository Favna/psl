{
  "name": "psl",
  "version": "1.8.0",
  "description": "Domain name parser based on the Public Suffix List",
  "repository": {
    "type": "git",
    "url": "git@github.com:lupomontero/psl.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "browser": "dist/index.umd.js",
  "unpkg": "dist/index.umd.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js"
  },
  "scripts": {
    "pretest": "eslint src",
    "test": "mocha test && karma start ./karma.conf.js --single-run",
    "watch": "mocha test --watch",
    "prebuild": "node ./scripts/update-rules.mjs",
    "build": "rollup -c",
    "changelog": "git log $(git describe --tags --abbrev=0)..HEAD --oneline --format=\"%h %s (%an <%ae>)\""
  },
  "keywords": [
    "publicsuffix",
    "publicsuffixlist"
  ],
  "files": [
    "dist"
  ],
  "author": "Lupo Montero <lupomontero@gmail.com> (https://lupomontero.com/)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "eslint": "^7.23.0",
    "eslint-config-hapi": "^12.0.0",
    "eslint-plugin-hapi": "^4.1.0",
    "JSONStream": "^1.3.5",
    "karma": "^6.3.2",
    "karma-browserify": "^8.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "mocha": "^8.3.2",
    "phantomjs-prebuilt": "^2.1.16",
    "request": "^2.88.2",
    "rollup": "^2.44.0",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.2"
  }
}
